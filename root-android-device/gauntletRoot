INDEX=0
for device in `adb devices 2>&1 | grep -v -e daemon -v -e devices | awk '{print $1}'`; do
let INDEX=${INDEX}+1
echo Device[${INDEX}] $device
done

read -p "Press [ENTER] to root $INDEX devices"

INDEX=0
for device in `adb devices 2>&1 | grep -v -e daemon -v -e devices | awk '{print $1}'`; do 
let INDEX=${INDEX}+1
bash rootTarget.sh $device $INDEX $1 &
done
